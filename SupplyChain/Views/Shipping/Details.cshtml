@model SupplyChain.Models.InventoryManagement

@{
    ViewData["Title"] = "Detalhes da Entrada";
}

<h1>Detalhes</h1>

<div>
    <h4>Saída de Mercadoria</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Produto
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Product.Name)
        </dd>
        <dt class = "col-sm-2">
            Saída
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreateAt)
        </dd>
        <dt class = "col-sm-2">
            Atualizado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UpdateAt)
        </dd>
        <dt class = "col-sm-2">
            Local
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Local)
        </dd>
        <dt class = "col-sm-2">
            Quantidade
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Quantity)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index" style="margin-right: 10px" class="btn btn-primary">Voltar</a>
</div>